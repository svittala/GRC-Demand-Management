                 var recordsQueryPosition = resultQueryPosition.getArray(&quot;records&quot;);
                  if (recordsQueryPosition != null &amp;&amp; recordsQueryPosition.length &gt; 0 )
                  {
                     // if (recordsQueryPosition[0].Status__c  == &quot;E-Filled&quot; )
